  xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/"
  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
  xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/"
  targetNamespace="http://Aeropuerto">
  <wsdl:documentation>
    Service: Servicio Aeropuerto
    Version: 1.0
    Owner: AST C3.
  </wsdl:documentation>
  <!-- Types element: -->
  <wsdl:types>
    <xsd:schema attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://Aeropuerto">
      <!-- Obtener la informacion del aeropuerto de origen y destino -->
      <xsd:element name="getInfoAeropuerto">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="ciudadOrigen" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="ciudadDestino" nillable="true" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="getInfoAeropuertoResponse">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="aeropuertosOrigenResponse" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="aeropuertosDestinoResponse" nillable="true" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <!-- END getInfoAeropuerto -->
    </xsd:schema>
  </wsdl:types>
  <!-- END Types element: -->
  <!-- Message element: -->
  <!-- input message of getInfoAeropuertoOrigen -->
  <wsdl:message name="getInfoAeropuertoSoapIn">
    <wsdl:part name="parameters" element="tns:getInfoAeropuerto"/>
  </wsdl:message>
  <!-- output message of getInfoAeropuertoOrigen -->
  <wsdl:message name="getInfoAeropuertoSoapOut">
    <wsdl:part name="parameters" element="tns:getInfoAeropuertoResponse"/>
  </wsdl:message>
  <!-- END Message element: -->
  <!-- portType element: -->
  <wsdl:portType name="AeropuertoPortType">
    <wsdl:documentation>Description of the business interface</wsdl:documentation>
    <!-- operation of getInfoAeropuertoOrigen -->
    <wsdl:operation name="getInfoAeropuertoOperation">
      <wsdl:input message="tns:getInfoAeropuertoSoapIn" wsaw:Action="urn:getInfoAeropuerto"/>
      <wsdl:output message="tns:getInfoAeropuertoSoapOut" wsaw:Action="urn:getInfoAeropuertoResponse"/>
    </wsdl:operation>
  </wsdl:portType>
  <!-- END portType element: -->
  <!-- binding elements -->
  <wsdl:binding name="AeropuertoSoap11Binding" type="tns:AeropuertoPortType">
      <soap:binding transport="http://schemas.xmlsoap.org/soap/http" style="document"/>
      <wsdl:operation name="getInfoAeropuertoOperation">
          <soap:operation soapAction="urn:getInfoAeropuertoOperation" style="document"/>
          <wsdl:input>
              <soap:body use="literal"/>
          </wsdl:input>
          <wsdl:output>
              <soap:body use="literal"/>
          </wsdl:output>
      </wsdl:operation>
  </wsdl:binding>
  <wsdl:binding name="AeropuertoSoap12Binding" type="tns:AeropuertoPortType">
      <soap12:binding transport="http://schemas.xmlsoap.org/soap/http" style="document"/>
      <wsdl:operation name="getInfoAeropuertoOperation">
          <soap12:operation soapAction="urn:getInfoAeropuertoOperation" style="document"/>
          <wsdl:input>
              <soap12:body use="literal"/>
          </wsdl:input>
          <wsdl:output>
              <soap12:body use="literal"/>
          </wsdl:output>
      </wsdl:operation>
  </wsdl:binding>
  <wsdl:binding name="AeropuertoHttpBinding" type="tns:AeropuertoPortType">
      <http:binding verb="POST"/>
      <wsdl:operation name="getInfoAeropuertoOperation">
          <http:operation location="getInfoAeropuertoOperation"/>
          <wsdl:input>
              <mime:content type="application/xml" part="parameters"/>
          </wsdl:input>
          <wsdl:output>
              <mime:content type="application/xml" part="parameters"/>
          </wsdl:output>
      </wsdl:operation>
  </wsdl:binding>
  <!-- END binding elements -->
  <!-- service element -->
  <wsdl:service name="Aeropuerto">
      <wsdl:port name="AeropuertoHttpSoap11Endpoint" binding="tns:AeropuertoSoap11Binding">
          <soap:address location="http://localhost:8080/axis2/services/Aeropuerto"/>
      </wsdl:port>
      <wsdl:port name="AeropuertoHttpSoap12Endpoint" binding="tns:AeropuertoSoap12Binding">
          <soap12:address location="http://localhost:8080/axis2/services/Aeropuerto"/>
      </wsdl:port>
      <wsdl:port name="AeropuertoHttpEndpoint" binding="tns:AeropuertoHttpBinding">
          <http:address location="http://localhost:8080/axis2/services/Aeropuerto"/>
      </wsdl:port>
  </wsdl:service>
  <!-- END service element -->
</wsdl:definitions>
